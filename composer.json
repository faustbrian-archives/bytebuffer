{
    "name": "kodekeep/bytebuffer",
    "description": "A fast and simple ByteBuffer implementation using SplFixedArray.",
    "keywords": [
        "kodekeep",
        "bytebuffer"
    ],
    "homepage": "https://github.com/kodekeep/bytebuffer",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.3",
        "symfony/var-dumper": "4.4"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\ByteBuffer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\ByteBuffer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=coverage.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\ByteBuffer\\ByteBufferServiceProvider"
            ],
            "aliases": {
                "ByteBuffer": "KodeKeep\\ByteBuffer\\ByteBufferFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
